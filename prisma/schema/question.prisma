model Question {
    id            Int        @id @default(autoincrement())
    questionName  String
    options       Options[]
    simulatorId   Int
    categoryId    Int
    answer        Int
    justification String?
    Simulator     Simulator? @relation(fields: [simulatorId], references: [id])
    category      Category   @relation(fields: [categoryId], references: [id])
}
